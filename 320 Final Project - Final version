   "source": [
    "# I now continue cleaning columns for the 5 year dataset. I am removing the \"\\r\" from the second column for ease of readability, as I did previously.\n",
    "\n",
    "five_year_trends[\"\\r\"] = five_year_trends[\"\\r\"].str.replace(\"\\r\",\"\")\n",
    "five_year_trends[\"Category: All categories\"] = five_year_trends[\"Category: All categories\"].str.replace(\"outfits with\",\"\")\n",
    "\n",
    "#Similar rows I will combine are: {5, 14, 17, 18} for jeans , {9, 24, 25} for sneakers, (6, 27} for boots, and {10, 23} for skirts \n",
    "\n",
    "#combining rows 5, 14, 17, 18 (all relate to jeans) at row 5 \n",
    "five_jeans = [int(five_year_trends.at[5, '\\r']), int(five_year_trends.at[14, '\\r']), int(five_year_trends.at[17, '\\r']), int(five_year_trends.at[18, '\\r'])]\n",
    "five_year_trends.at[5, '\\r'] = sum(five_jeans)\n",
    "\n",
    "#deleting rows: 14, 17, 18 since they are now accounted for at row 5\n",
    "five_year_trends = five_year_trends.drop([14, 17, 18], axis=0)\n",
    "\n",
    "#combining rows 9, 24, 25 (all relate to sneakers) at row 9\n",
    "five_jeans = [int(five_year_trends.at[9, '\\r']), int(five_year_trends.at[24, '\\r']), int(five_year_trends.at[25, '\\r'])]\n",
    "five_year_trends.at[9, '\\r'] = sum(five_jeans)\n",
    "\n",
    "#renaming row 9 as \"sneakers\" since that is the grouping factor\n",
    "five_year_trends.at[9, 'Category: All categories'] = \"sneakers\"\n",
    "\n",
    "#deleting rows: 24, 25 since they are now accounted for in row 9\n",
    "five_year_trends = five_year_trends.drop([24, 25], axis=0)\n",
    "\n",
    "#combining rows 6, 27 (all relate to boots) at row 6 \n",
    "five_jeans = [int(five_year_trends.at[6, '\\r']), int(five_year_trends.at[27, '\\r'])]\n",
    "five_year_trends.at[6, '\\r'] = sum(five_jeans)\n",
    "\n",
    "#deleting rows: 27 since it is now accounted for in row 6\n",
    "five_year_trends = five_year_trends.drop([27], axis=0)\n",
    "\n",
    "#combining rows 10, 23 (all relate to skirts) at row 10\n",
    "five_jeans = [int(five_year_trends.at[10, '\\r']), int(five_year_trends.at[23, '\\r'])]\n",
    "five_year_trends.at[10, '\\r'] = sum(five_jeans)\n",
    "\n",
    "#deleting rows: 23 since it is now accounted for in row 10\n",
    "five_year_trends = five_year_trends.drop([23], axis=0)\n",
    "print(five_year_trends)"
   ]
